{
    "name": "piefingers",
    "version": "1.0.0",
    "description": "Search multiple music platforms with a single search!",
    "main": "app.js",
    "scripts": {
        "dev": "nodemon server.js",
        "start:prod": "NODE_ENV=production nodemon server.js",
        "debug": "ndb server.js",
        "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
        "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
        "watch:sass": "node-sass sass/main.scss public/css/style.css -w",
        "devserver": "live-server",
        "start:css": "npm-run-all --parallel devserver watch:sass",
        "compile:sass": "node-sass sass/main.scss public/css/style.comp.css",
        "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/css/style.comp.css -o public/css/style.prefix.css",
        "compress:css": "node-sass public/css/style.prefix.css public/css/style.css --output-style compressed",
        "build:css": "npm-run-all compile:sass prefix:css compress:css"
    },
    "author": "Oliver Stuart",
    "license": "ISC",
    "dependencies": {
        "@babel/polyfill": "^7.10.4",
        "axios": "^0.20.0",
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "cookie-session": "^1.4.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-flash": "0.0.2",
        "express-mongo-sanitize": "^2.0.0",
        "express-rate-limit": "^5.1.3",
        "express-session": "^1.17.1",
        "googleapis": "^61.0.0",
        "helmet": "^4.0.0",
        "hpp": "^0.2.3",
        "html-to-text": "^5.1.1",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.9.25",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
        "node-sass": "^4.14.1",
        "node-soundcloud": "0.0.6",
        "nodemailer": "^6.4.11",
        "pug": "^3.0.0",
        "querystring": "^0.2.0",
        "request": "^2.88.2",
        "sharp": "^0.26.0",
        "slugify": "^1.4.5",
        "spotify-web-api-node": "^4.0.0",
        "validator": "^13.1.1",
        "xss-clean": "^0.1.1"
    },
    "devDependencies": {
        "eslint": "^7.5.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.3",
        "ndb": "^1.1.5",
        "parcel-bundler": "^1.12.4",
        "prettier": "^2.0.5"
    },
    "engines": {
        "node": "^12"
    }
}
